{
  "name": "diy",
  "description": "Do it yourself app",
  "version": "0.0.2",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/herau/diy-project.git"
  },
  "scripts": {
    "prebuild": "ncp src/main/resources/static/semantic/site node_modules/semantic-ui-less/site && ncp src/main/resources/static/semantic/theme.config node_modules/semantic-ui-less/theme.config",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build-watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "build-server": "webpack-dev-server --inline --progress --profile --colors --display-error-details --display-cached --port 3000",
    "webpack": "webpack",
    "doc": "typedoc  --options src/main/resources/static/typedoc.json  src/main/resources/static/app/**/*.ts",
    "clean": "rimraf node_modules src/main/resources/static/doc && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "preprotractor": "npm run webdriver-update",
    "protractor": "protractor",
    "e2e": "npm run protractor",
    "test": "karma start src/test/js/karma.conf.js",
    "start": "mvn spring-boot:run"
  },
  "dependencies": {
    "semantic-ui-less": "^2.1.4",
    "angular2": "2.0.0-alpha.53",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-alpha.14",
    "zone.js": "0.5.8",
    "core-js": "^1.2.6"

  },
  "devDependencies": {
    "css-loader": "^0.23.0",
    "less-loader": "^2.2.1",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.3",
    "url-loader": "^0.5.6",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.0",
    "karma": "^0.13.11",
    "karma-chrome-launcher": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "protractor": "^3.0.0",
    "object-assign": "^4.0.1",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.3",
    "ts-loader": "^0.7.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1",
    "rimraf": "^2.4.4",
    "ncp": "^2.0.0"
  }
}
