{
  "name": "diy",
  "description": "Do it yourself app",
  "version": "0.0.2",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/herau/diy-project.git"
  },
  "scripts": {
    "prebuild": "ncp src/main/resources/static/semantic/site node_modules/semantic-ui-less/site && ncp src/main/resources/static/semantic/theme.config node_modules/semantic-ui-less/theme.config",
    "build": "webpack --colors --display-error-details --display-cached",
    "build-watch": "webpack --watch",
    "build-server": "webpack-dev-server --inline --colors --display-error-details --display-cached --port 3000",
    "webpack": "webpack",
    "clean": "rimraf node_modules tsd_typings",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "preprotractor": "npm run webdriver-update",
    "protractor": "protractor",
    "e2e": "npm run protractor",
    "test": "karma start src/test/js/karma.conf.js",
    "remove-tsd-loader-typings": "rimraf node_modules/typescript-simple-loader/typescript-simple-loader.d.ts",
    "tsd-update": "npm run remove-tsd-loader-typings && tsd reinstall --overwrite",
    "postinstall": "npm run tsd-update && tsd install && tsd link",
    "start": "mvn spring-boot:run"
  },
  "dependencies": {
    "angular2": "2.0.0-alpha.37",
    "semantic-ui-less": "^2.1.4",
    "reflect-metadata": "^0.1.1",
    "rx": "^2.5.3",
    "zone.js": "^0.5.4"
  },
  "devDependencies": {
    "css-loader": "^0.18.0",
    "less-loader": "^2.2.1",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.0",
    "file-loader": "^0.8.1",
    "imports-loader": "^0.6.3",
    "json-loader": "^0.5.1",
    "url-loader": "^0.5.5",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.12.2",
    "karma": "^0.13.3",
    "karma-chrome-launcher": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-coverage": "^0.5.0",
    "karma-jasmine": "^0.3.5",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.18",
    "protractor": "^2.1.0",
    "object-assign": "^4.0.1",
    "tsd": "^0.6.4",
    "typescript": "^1.6.2",
    "typescript-simple-loader": "^0.3.7",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1",
    "rimraf": "^2.4.3",
    "ncp": "^2.0.0"
  }
}
